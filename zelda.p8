    for i=1,#gems do
      local sprite_offset = gems[i].gem_type
      local frame_time = 0.5
      -- set stage
      local stage = flr(t() / frame_time % 5)
      gems[i].frame = stage + sprite_offset
      if(hit_obj(gems[i].x, gems[i].y)) then
        gems[i].x = -1
        gems[i].y = -1
        gemScore += 1
      end
    end
  end
