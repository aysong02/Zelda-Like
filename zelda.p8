--code for player movement
  function input()
    local dx, dy = 0,0
    if(btn(0)) then --left
      dx = -speed * dt
      player.orientation = 232
    elseif (btn(1)) then --right
      dx = speed * dt
      player.orientation = 200
    elseif (btn(2)) then --up
      dy = -speed * dt
      player.orientation = 224
    elseif (btn(3)) then --down
      dy= speed * dt
      player.orientation = 192
    end

    if hit(player.x+dx,player.y,player.w,player.h,player.ox,player.oy) then
      dx = 0
    end
    if hit(player.x,player.y+dy,player.w,player.h,player.ox,player.oy) then
      dy = 0
    end

    player.x += dx
    player.y += dy
    mapp.x += dx
    mapp.y += dy
  end
  --code for wall collisions
  --Checks for collisions on all sides of the sprite
  function hit(x,y,w,h,ox,oy)
    collide=false
    for i=x+ox,x+w+ox do
      --Check both the top and the bottom 
      if (fget(mget(i/8,y+oy/8),7)) or
          (fget(mget(i/8,(y+h+oy)/8),7)) then
            collide=true
      end
    end
    
    for i=y+oy,y+h+oy do
      --Check both the left and the right 
      if (fget(mget(x+ox/8,i/8))>0) or
          (fget(mget((x+w+ox)/8,i/8))>0) then
            collide=true
      end
    end
    
    return collide
  end
  
  function hit_obj(x,y)
  --x,y for trap and uses player object
  -- TODO refactor code to be faster
    collide=false
    for i=x,x+8,8 do
      for j=y,y+8,8 do
        if i >= player.x and i <= player.x + player.w and
        j >= player.y and j <= player.y + player.h then
          collide = true
        end
      end
    end
    return collide
  end
