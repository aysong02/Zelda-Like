--code for spikes
  function draw_spikes()
    for i=1,#spikes do
      if(
        spikes[i].x >= mapp.x - 10
        and spikes[i].x <= (mapp.x + mapp.width + 10) 
        and spikes[i].y >= mapp.y - 10
        and spikes[i].y <= (mapp.y + mapp.hight + 10)
        ) then
       spr(spikes[i].frame, spikes[i].x, spikes[i].y, 2, 2)
      end
    end
  end

  function plant_spikes(x,y)
    local seed = {
      x=x*8,
      y=y*8,
      frame=0
    }
    spike_count += 1
    spikes[spike_count] = seed
  end

  function plant_all_spikes()
    for i=1, #spike_locations do
      plant_spikes(spike_locations[i][1], spike_locations[i][2])
    end
  end

  function update_spikes()
    trap_hit = false
    for i=1,#spikes do
      local sprite_offset = 32
      local frame_time = 2
      -- set stage
      local stage = flr(t() / frame_time % 3)*2
      spikes[i].frame = stage + sprite_offset
      if (stage == 4) then
        trap_hit = trap_hit or hit_obj(spikes[i].x, spikes[i].y)
      end
    end
    if trap_hit then
      if (player.alive == true) then 
        sfx(2)
      end
      player.alive = false
    end
  end
