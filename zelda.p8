-- special functions
  -- code modified from class rain.p8
  function _init() 
    music(0)
    starting_position = {
      x = 49 * 8,
      y = 39 * 8,
    }
    player = {
      --player coordinates
      x = starting_position.x,
      y = starting_position.y,
      --offset for sprite from top left
      ox=0, 
      oy=0,
      --sprite dimensions
      w = 15,
      h = 15,
      orientation = 192,
    -- orientation is 0,2,4,6 for
    -- left, right, up, down
      alive = true,
    }

    mapp = {
      x = starting_position.x-64,
      y = starting_position.y-64,
      width = 128,
      hight = 128
    }

    -- initailize spikes and gems
    spikes = {}
    trap_hit = false
    spike_count = 0
    plant_all_spikes()
    gems = {}
    gemCount = 0
    gemScore = 0
    plant_all_gems()

    
    flames = {}
    flame_count = 0
    plant_flames(110,26)
    speed = 80 -- pixels/sec
    dx, dy = 0, 0
    dt = 0
    lastframe = t()
  end

  function _draw()
    cls()
    camera(mapp.x, mapp.y)
    map()
    draw_spikes()
    draw_gem()
    draw_flames()
    if player.alive then
      spr(player.orientation + flr((t()*1.5) % 4)*2, player.x, player.y, 2, 2)
    end
    print(player.alive, mapp.x + 2, mapp.y + 120, 12)
   print("gems: "..gemScore, mapp.x + 50, mapp.y + 120, 11)
  end

  function _update60()
    dt = t() - lastframe
    lastframe = t()

    if player.alive then
      input() 
    end
    update_spikes()
    update_gems()
    update_flames()
  end





