  function _draw()
    cls()
    camera(mapp.x, mapp.y)
    map()
    draw_spikes()
    draw_gem()
    if player.alive then
      spr(player.orientation, player.x, player.y, 2, 2)
    end
    print(player.alive, mapp.x + 2, mapp.y + 120, 12)
    print("gems: "..gemScore, mapp.x + 50, mapp.y + 120, 11)
  end

  function _update60()
    dt = t() - lastframe
    lastframe = t()

    if player.alive then
      input() 
    end
    update_spikes()
    update_gems()
  end





